{{render "mapLocation" mapLocation}}
{{view "map" id="map"}}
<div class="logo">
    <img src="assets/images/canimap_cross_on_dog.png"/>
</div>
<div class="top-container btn-group">
  {{render "mapDataLt" controller="mapData"}}
  {{render "mapDrawLt" controller="mapDraw"}}
  <div class="mtg-mode">
    <a role="button" class="btn btn-success radius-right" data-toggle="tooltip"
       title="Switch to expert mode" href="/expert">
        Expert
    </a>
  </div>
  {{view "mapLayers"}}
</div>

